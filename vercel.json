{
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/pages/api/bot.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/cron",
      "dest": "/pages/api/cron.js",
      "methods": ["GET"]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/pages/api/:1"
    }
  ]
}
