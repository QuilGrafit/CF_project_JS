{
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/pages/api/bot.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/cron",
      "dest": "/pages/api/cron.js",
      "methods": ["GET"]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/pages/api/:1"
    }
  ]
  "dependencies": {
    "next": "^14.0.0",
    "telegraf": "^4.12.2",
    "mongodb": "^5.7.0",
    "i18next": "^23.7.8",
    "typescript": "^5.2.2"
  }
}
